<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:ff="clr-namespace:FFImageLoading.Forms;assembly=FFImageLoading.Forms"
             x:Class="Avalanche.Layouts.FullWidth">
  <ContentView.Content>
        <StackLayout Spacing="0">
            <StackLayout x:Name="Feature" Spacing="0">
            </StackLayout>
            <StackLayout x:Name="Main" Spacing="0">
            </StackLayout>
            <StackLayout x:Name="SectionA" Spacing="0">
            </StackLayout>
            <StackLayout Orientation="Horizontal" Spacing="0">
                <StackLayout x:Name="SectionB" Spacing="0">
                </StackLayout>
                <StackLayout x:Name="SectionC" Spacing="0">
                </StackLayout>
                <StackLayout x:Name="SectionD" Spacing="0">
                </StackLayout>
            </StackLayout>
        </StackLayout>
  </ContentView.Content>
</ContentView>